{
  "compilerOptions": {
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "declarationDir": "./dist",
    "outDir": "./dist",
    "preserveConstEnums": false,
    "removeComments": true,
    "target": "es6",
    "module": "commonJs",
    "stripInternal": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "declaration": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
